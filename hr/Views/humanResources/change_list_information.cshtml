
@{
    Layout = null;
}
@using hrModel;
@model hrModel.human_file
<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <link rel="stylesheet"
          href="../../css/table.css" type="text/css">
    <link rel="stylesheet"
          href="../../css/cwcalendar.css" type="text/css">
    <script type="text/javascript"
            src="../../javascript/comm/comm.js">
    </script>
    <script language="javascript"
            src="../../javascript/winopen/winopenm.js">
    </script>
    <script type="text/javascript"
            src="../../javascript/calendar-ch.js">
    </script>
    <script type="text/javascript"
            src="../../javascript/jquery-1.7.2.js">
    </script>
    <script type="text/javascript"
            src="../../javascript/jquery.messager.js">
    </script>
    <script src="../../javascript/time.js"></script>
    <script type="text/javascript"
            src="../../javascript/human_check.js">
    </script>
    <script type="text/javascript"
            src="../../javascript/human_input_check.js">
    </script>
</head>
<body>

    <form action="../register_choose_picture" method="post" enctype="multipart/form-data">
        <input id="Hidden1" type="hidden" name="huf_id" value="@Model.huf_id" />
        <input id="Hidd" type="hidden" name="check_status" value="2" />
        <table width="100%">
            <tr>
                <td>
                    <font color="black">您正在做的业务是：人力资源--人力资源档案管理--人力资源档案变更 </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="submit" value="确认变更" class="BUTTON_STYLE1" />
                    <input type="reset" value="清除" class="BUTTON_STYLE1">
                    <input type="button" value="返回" class="BUTTON_STYLE1"
                           onclick="history.back()">
                </td>
            </tr>
        </table>
        <table width="100%" border="1" cellpadding=3 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr>
                <td class="TD_STYLE1" width="10%">
                    档案编号
                </td>
                <td colspan="6" class="TD_STYLE2">
                    <input id="Hidden1" type="hidden" name="human_id" value="@Model.human_id" />
                    @Model.human_id
                </td>
                <td rowspan="6" width="13%" style="text-align: center;">
                    <img src="../../images/regular.jpg" style="width:120px;height:150px;" />
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1" width="10%">
                    I级机构
                </td>
                <td width="13%" class="TD_STYLE2">
                    <input id="Hidden1" type="hidden" name="first_kind_id" value="@Model.first_kind_id" />
                    <input id="Hidden1" type="hidden" name="first_kind_name" value="@Model.first_kind_name" />
                    @Model.first_kind_name
                </td>
                <td width="10%" class="TD_STYLE1">
                    II级机构
                </td>
                <td width="13%" class="TD_STYLE2">
                    <input id="Hidden1" type="hidden" name="second_kind_id" value="@Model.second_kind_id" />
                    <input id="Hidden1" type="hidden" name="second_kind_name" value="@Model.second_kind_name" />
                    @Model.second_kind_name
                </td>
                <td width="10%" class="TD_STYLE1">
                    III级机构
                </td>
                <td width="13%" class="TD_STYLE2">
                    <input id="Hidden1" type="hidden" name="third_kind_id" value="@Model.third_kind_id" />
                    <input id="Hidden1" type="hidden" name="third_kind_name" value="@Model.third_kind_name" />
                    @Model.third_kind_name
                </td>
                <td class="TD_STYLE2" colspan="2" width="2%"></td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    职位分类
                </td>
                <td class="TD_STYLE2">
                    <input id="Hidden1" type="hidden" name="human_major_kind_id" value="@Model.human_major_kind_id" />
                    <input id="Hidden1" type="hidden" name="human_major_kind_name" value="@Model.human_major_kind_name" />
                    @Model.human_major_kind_name
                </td>
                <td class="TD_STYLE1">
                    职位名称
                </td>
                <td class="TD_STYLE2">
                    <input id="Hidden1" type="hidden" name="human_major_id" value="@Model.human_major_id" />
                    <input id="Hidden1" type="hidden" name="hunma_major_name" value="@Model.hunma_major_name" />
                    @Model.hunma_major_name
                </td>
                <td class="TD_STYLE1">
                    职称
                </td>
                <td colspan="2" class="TD_STYLE2">
                    <input id="Hidden1" type="hidden" name="human_major_id" value="@Model" />
                    <select class="SELECT_STYLE1">
                        <option value="工程师">工程师</option>
                        <option value="助理">助理</option>
                        <option value="经理">经理</option>
                    </select>
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    姓名
                </td>
                <td class="TD_STYLE2">
                    <input type="text" id="humanName" name="human_name" value="@Model.human_name"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    性别
                </td>
                <td class="TD_STYLE2">
                    <select class="SELECT_STYLE1" name="human_sex">
                        <option>男</option>
                        <option>女</option>
                    </select>
                </td>
                <td class="TD_STYLE1">
                    EMAIL
                </td>
                <td colspan="2" class="TD_STYLE2">
                    <input type="text" name="human_email" value="@Model.human_email"
                           class="INPUT_STYLE2" id="humanEmail">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    电话
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_telephone" value="@Model.human_telephone"
                           class="INPUT_STYLE2" id="humanTelephone">
                </td>
                <td class="TD_STYLE1">
                    QQ
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_qq" value="@Model.human_qq"
                           class="INPUT_STYLE2" id="humanQq">
                </td>
                <td class="TD_STYLE1">
                    手机
                </td>
                <td colspan="2" class="TD_STYLE2">
                    <input type="text" name="human_mobilephone" id="humanMobilephone"
                           value="@Model.human_mobilephone" class="INPUT_STYLE2">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    住址
                </td>
                <td colspan="3" class="TD_STYLE2">
                    <input type="text" name="human_address" value="@Model.human_address"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    邮编
                </td>
                <td colspan="2" class="TD_STYLE2">
                    <input type="text" name="human_postcode" value="@Model.human_postcode"
                           class="INPUT_STYLE2">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    国籍
                </td>
                <td class="TD_STYLE2">
                    <select name="human_nationality" class="SELECT_STYLE1">
                        @foreach (var item in ViewData["gjz"] as List<config_public_char>)
                        {
                            if (item.attribute_kind == "国籍")
                            {
                                <option value="@item.attribute_name">@item.attribute_name</option>
                            }
                        }
                    </select>
                </td>
                <td class="TD_STYLE1">
                    出生地
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_birthplace" value="@Model.human_birthplace "
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    生日
                </td>
                <td width="13%" class="TD_STYLE2">
                    <input type="text" name="human_birthday" readonly="readonly"
                           value="@Model.human_birthday" class="INPUT_STYLE2" id="birthday">
                </td>
                <td width="10%" class="TD_STYLE1">
                    民族
                </td>
                <td class="TD_STYLE2">
                    <select name="human_race" class="SELECT_STYLE1">
                        @foreach (var item in ViewData["gjz"] as List<config_public_char>)
                        {
                            if (item.attribute_kind == "民族")
                            {
                                <option value="@item.attribute_name">@item.attribute_name</option>
                            }
                        }
                    </select>
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    宗教信仰
                </td>
                <td class="TD_STYLE2">
                    <select name="human_religion" class="SELECT_STYLE1">
                        @foreach (var item in ViewData["gjz"] as List<config_public_char>)
                        {
                            if (item.attribute_kind == "宗教信仰")
                            {
                                <option value="@item.attribute_name">@item.attribute_name</option>
                            }
                        }
                    </select>
                </td>
                <td class="TD_STYLE1">
                    政治面貌
                </td>
                <td class="TD_STYLE2">
                    <select name="human_party" class="SELECT_STYLE1">
                        @foreach (var item in ViewData["gjz"] as List<config_public_char>)
                        {
                            if (item.attribute_kind == "政治面貌")
                            {
                                <option value="@item.attribute_name">@item.attribute_name</option>
                            }
                        }
                    </select>
                </td>
                <td class="TD_STYLE1">
                    身份证号码
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_id_card" id="humanIdCard"
                           value="@Model.human_id_card" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    社会保障号码
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_society_security_id" value="@Model.human_society_security_id"
                           class="INPUT_STYLE2">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    年龄
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_age" value="@Model.human_age"
                           class="INPUT_STYLE2" id="humanAge">
                </td>
                <td class="TD_STYLE1">
                    学历
                </td>
                <td class="TD_STYLE2">
                    <select name="human_educated_degree" class="SELECT_STYLE1">
                        @foreach (var item in ViewData["gjz"] as List<config_public_char>)
                        {
                            if (item.attribute_kind == "学历")
                            {
                                <option value="@item.attribute_name">@item.attribute_name</option>
                            }
                        }
                    </select>
                </td>
                <td class="TD_STYLE1">
                    教育年限
                </td>
                <td class="TD_STYLE2">
                    <select name="human_educated_years" class="SELECT_STYLE1">
                        @foreach (var item in ViewData["gjz"] as List<config_public_char>)
                        {
                            if (item.attribute_kind == "教育年限")
                            {
                                <option value="@item.attribute_name">@item.attribute_name</option>
                            }
                        }
                    </select>
                </td>
                <td class="TD_STYLE1">
                    学历专业
                </td>
                <td class="TD_STYLE2">
                    <select name="human_educated_major" class="SELECT_STYLE1">
                        @foreach (var item in ViewData["gjz"] as List<config_public_char>)
                        {
                            if (item.attribute_kind == "专业")
                            {
                                <option value="@item.attribute_name">@item.attribute_name</option>
                            }
                        }
                    </select>
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    薪酬标准
                </td>
                <td class="TD_STYLE2">
                    @Model.salary_standard_name
                    <input type="hidden" name="salary_standard_name"
                           value="@Model.salary_standard_name">
                </td>
                <td class="TD_STYLE1">
                    开户行
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_bank" value="@Model.human_bank"
                           class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    帐号
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="human_account"
                           value="@Model.human_account" class="INPUT_STYLE2">
                </td>
                
            </tr>
            <tr>
                
                <td class="TD_STYLE1">
                    特长
                </td>
                <td class="TD_STYLE2">
                    <select name="human_speciality" class="SELECT_STYLE1">
                        @foreach (var item in ViewData["gjz"] as List<config_public_char>)
                        {
                            if (item.attribute_kind == "特长")
                            {
                                <option value="@item.attribute_name">@item.attribute_name</option>
                            }
                        }
                    </select>
                </td>
                <td class="TD_STYLE1">
                    爱好
                </td>
                <td class="TD_STYLE2">
                    <select name="human_hobby" class="SELECT_STYLE1">
                        @foreach (var item in ViewData["gjz"] as List<config_public_char>)
                        {
                            if (item.attribute_kind == "爱好")
                            {
                                <option value="@item.attribute_name">@item.attribute_name</option>
                            }
                        }
                    </select>
                </td>
                <td class="TD_STYLE1">
                    &nbsp;
                </td>
                <td class="TD_STYLE2">
                    &nbsp;
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    档案附件
                </td>
                <td colspan="7" class="TD_STYLE2">
                    <a href="javascript:winopenm('query.jsp?filename=')">@Model.attachment_name</a>
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    个人履历
                </td>
                <td colspan="7" class="TD_STYLE2">
                    <textarea name="human_histroy_records" rows="4"
                              class="TEXTAREA_STYLE1" style="resize:none;width:99%;">@Model.human_histroy_records</textarea>
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    家庭关系信息
                </td>
                <td colspan="7" class="TD_STYLE2">
                    <textarea name="human_family_membership" rows="4"
                              class="TEXTAREA_STYLE1" style="resize:none;width:99%;">@Model.human_family_membership</textarea>
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    备注
                </td>
                <td colspan="7" class="TD_STYLE2">
                    <textarea name="human_family_membership" rows="4"
                              class="TEXTAREA_STYLE1" style="resize:none;width:99%;">@Model.remark</textarea>
                </td>
            </tr>
        </table>
    </form>
</body>
</html>
